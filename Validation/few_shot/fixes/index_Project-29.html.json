        "error_fixes": [
            {
                "problem_type": "alt attribute is missing in iframe or img tag",
                "offending_line": "<img src=\"chicken_biryani.jpg\" alt=\"Chicken Biryani\">",
                "fixed_line": "<img src=\"chicken_biryani.jpg\" alt=\"Chicken Biryani\">"
            },
            {
                "problem_type": "Alt attribute is missing in iframe tag",
