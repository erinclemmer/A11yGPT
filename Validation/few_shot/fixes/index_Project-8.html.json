            {
                "problem": "<font size=\"5\">Description of the book...</font>",
                "solution": "<p>Description of the book...</p>"
            },
            {
                "problem": "<b>Important</b>",
                "solution": "<strong>Important</strong>"
            },
            {
                "problem": "<i>Italic</i>",
                "solution": "<em>Italic</em>"
            }
        ],
        "error_fixes": [
            {
                "problem_type": "Use of <i> tag for styling",
                "offending_line": "<i>Italic</i>",
                "fixed_line": "<em>Italic</em>"
            },
            {
                "problem_type": "Use of <b> tag for bold text",
                "offending_line": "<b>Important</b>",
                "fixed_line": "<strong>Important</strong>"
            },
            {
                "problem_type": "Use of <i> tag for italic text",
                "offending_line": "<i>Discover powerful performance and stunning design with our new range of DELL laptops. Whether you're a student, professional, or gamer, we have the perfect laptop for you.</i>",
                "fixed_line": "<em>Discover powerful performance and stunning design with our new range of DELL laptops. Whether you're a student, professional, or gamer, we have the perfect laptop for you.</em>"
            }
        ]
    },
